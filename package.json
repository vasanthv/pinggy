{
	"name": "pinggy",
	"version": "0.2.6",
	"description": "An ultra minimal rss reader to follow multiple sources.",
	"main": "init.js",
	"scripts": {
		"start": "node ./scripts/pre-start.js && node init.js",
		"start-scheduler": "node init-scheduler.js",
		"api-dev": "DISABLE_CSRF=true node init.js",
		"gen:vapid": "node ./scripts/generate-vapid-keys.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"rss",
		"news",
		"atom",
		"reader"
	],
	"author": "Vasanth.V <imvasanthv@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-ses": "^3.556.0",
		"@mozilla/readability": "^0.5.0",
		"axios": "^1.7.7",
		"body-parser": "^1.20.2",
		"connect-mongo": "^5.1.0",
		"cron": "^3.1.7",
		"ejs": "^3.1.10",
		"eslint": "^9.13.0",
		"express": "^4.19.2",
		"express-rate-limit": "^7.2.0",
		"express-session": "^1.18.0",
		"express-slow-down": "^2.0.1",
		"generate-api-key": "^1.0.2",
		"html-to-text": "^9.0.5",
		"isomorphic-dompurify": "^2.7.0",
		"jsdom": "^24.0.0",
		"mongoose": "^8.3.2",
		"morgan": "^1.10.0",
		"mustache": "^4.2.0",
		"postmark": "^4.0.5",
		"randomstring": "^1.3.0",
		"rss-parser": "^3.13.0",
		"uuid": "^9.0.1",
		"vue": "^3.4.25"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.12.0",
		"prettier": "3.4.2"
	},
	"prettier": {
		"printWidth": 120,
		"singleQuote": false,
		"trailingComma": "es5",
		"tabWidth": 2,
		"useTabs": true,
		"overrides": [
			{
				"files": [
					"*.ejs"
				],
				"options": {
					"parser": "html"
				}
			}
		]
	}
}
